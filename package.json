  "license": "GPL-3.0",
  "private": true,
  "engine": {
    "node": ">=14 <15"
  },
  "scripts": {
    "watch": "cross-env NODE_ENV=development tsup src/main.ts --watch --onSuccess \"node .next/api/main.js\"",
    "build:clean": "rimraf .next",
